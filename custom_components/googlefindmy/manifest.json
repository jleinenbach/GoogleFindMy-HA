{
  "domain": "googlefindmy",
  "name": "Google Find My Device",
  "after_dependencies": ["recorder"],
  "codeowners": ["@BSkando"],
  "config_flow": true,
  "dependencies": ["http"],
  "discovery": {
    "version": 1,
    "channels": [
      {
        "name": "cloud_scan",
        "source": "cloud_discovery",
        "description": "Cloud-triggered config flows using the Google Find My Device scan API"
      },
      {
        "name": "auth_file",
        "source": "auth_secrets",
        "description": "Local Auth/secrets.json watcher emitting discovery flows"
      }
    ]
  },
  "discovery_update_info": {
    "version": 1,
    "channels": [
      {
        "name": "cloud_scan",
        "description": "Discovery updates forwarded when Google cloud payloads change"
      },
      {
        "name": "auth_file",
        "description": "Updated Auth/secrets.json bundles mapped onto existing entries"
      }
    ]
  },
  "documentation": "https://github.com/BSkando/GoogleFindMy-HA",
  "integration_type": "device",
  "iot_class": "cloud_polling",
  "issue_tracker": "https://github.com/BSkando/GoogleFindMy-HA/issues",
  "loggers": ["custom_components.googlefindmy"],
  "requirements": [
    "aiohttp>=3.11.8",
    "beautifulsoup4>=4.12.3",
    "cryptography>=43.0.3",
    "ecdsa>=0.19.1",
    "gpsoauth>=2.0.0",
    "h2>=4.1.0",
    "http-ece>=1.2.1",
    "httpx[http2]>=0.28.0",
    "protobuf>=6.32.0",
    "pycryptodomex>=3.23.0",
    "pyscrypt>=1.6.2",
    "selenium>=4.37.0",
    "undetected_chromedriver>=3.5.5"
  ],
  "version": "1.6.0"
}
